@model List<EmployeeDetails.Models.Employee>


@{
    Layout = "~/Views/Shared/_Employee.cshtml";
}


<html>
<head>
    <link href="~/css/Table.css" rel="stylesheet">
</head>
<body>

<table>
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Date of Birth</th>
        <th>Phone No:</th>
        <th>Description</th>
        <th></th>
        
    </tr>
    <tr>
        @foreach (var employee in Model)
        {
            <tr>
                <td>@employee.EmployeeId</td>
                <td>@employee.EmployeeName</td>
                <td>@employee.EmployeeDOB</td>
                <td>@employee.EmployeePh</td>
                <td>@employee.Description</td>
                <td><a href="/Employee/View/@employee.EmployeeId">View</a></td>
            </tr>
        }
    </tr>
</table>

    <a href="/Home/Index" style=" margin-left: 70px;">
        <span>Back to Dashboard</span>
    </a>
    <a href="/Employee/Add" style=" margin-left: 60px;">
        <span>Create new Employee</span>
    </a>
</body>
</html>


